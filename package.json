    "react-bootstrap": "^2.10.0",
    "react-dom": "^18",
    "react-router-dom": "^6.22.0",
    "react-slick": "^0.30.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "postcss": "^8",
    "sharp": "^0.34.5",
